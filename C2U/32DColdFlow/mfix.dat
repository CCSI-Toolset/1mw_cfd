  SPECIES_EQ(1)         = .FALSE.    

! All phases
!-----------------------------------------------------------------------
  ENERGY_EQ             = .FALSE.                      

#_______________________________________________________________________
#
# FILTERED MODEL    
#_______________________________________________________________________                                                              

  DRAG_TYPE    = 'WEN_YU'           !Only drag formulation valid with SUBGRID model

  SUBGRID_TYPE = 'IGCI'             !SubGrid model from Princeton/Igci, the “old” Filter model
  !SUBGRID_TYPE = 'MILIOLI'          !SubGrid model from Princeton/Igci, the “old” Filter model

#_______________________________________________________________________
#
# GEOMETRY
#_______________________________________________________________________                                                              
                                                                               
  COORDINATES           = 'cylindrical' 
  XLENGTH               = 0.0685                        !radius
  IMAX                  = 24                            !cells in i direction
  YLENGTH               = 1.003                         !height
  JMAX                  = 360                           !cells in j direction
  NO_K                  = .TRUE.                        !2D, no k direction

  NODESI = 2   NODESJ = 12   NODESK = 1 !The decomposition in I, J, and K directions for a Distributed Memory Parallel machine

#_______________________________________________________________________
#
# NUMERICAL OPTIONS
#_______________________________________________________________________   

