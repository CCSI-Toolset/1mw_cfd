#_______________________________________________________________________
#
# RUN_CONTROL
#_______________________________________________________________________                                                              
                                                         
                                                                               
  RUN_NAME              = 'ColdFlow'             
  DESCRIPTION           = 'non-reacting fluidized bed'          
  RUN_TYPE              = 'new'             
  UNITS                 = 'SI'
  TIME                  = 0.0                           !start time
  TSTOP                 = 200.0
  DT                    = 1.0E-2                        !time step

  CALL_USR = .T.

#_______________________________________________________________________
#
# EQUATIONS TO SOLVE
#_______________________________________________________________________                                                              

! Gas Phase: 
!-----------------------------------------------------------------------
